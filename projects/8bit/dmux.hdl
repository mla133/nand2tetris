/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP dmux {
    IN in, sel;
    OUT a, b;

    PARTS:
	not(in=sel, out=notsel);
	and(a=in, b=notsel, out=a);
	and(a=in, b=sel, out=b);
}
