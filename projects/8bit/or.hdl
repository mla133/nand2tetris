 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP or {
    IN a, b;
    OUT out;

    PARTS:
	not(in=a, out=nota);
	not(in=b, out=notb);
	Nand(a=nota, b=notb, out=out);
}
