/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP xor {
    IN a, b;
    OUT out;

    PARTS:
	not(in=a, out=nota);
	not(in=b, out=notb);
	and(a=a, b=notb, out=aANDnotb);
	and(a=nota, b=b, out=notaANDb);
	or(a=aANDnotb, b=notaANDb, out=out);	
}
