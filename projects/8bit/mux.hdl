/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	not(in=sel, out=notsel);
	and(a=a, b=notsel, out=selA);
	and(a=sel, b=b, out=selB);
	or(a=selA, b=selB, out=out);
}
